{
  "name": "chatgptree",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run --browser.headless",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "test:storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm run build-storybook --quiet && pnpx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && pnpm run test:storybook\""
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "executionEnv": {
      "nodeVersion": "24.3.0"
    }
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.10",
    "@xyflow/react": "^12.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "scheduler": "^0.26.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "use-context-selector": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/addon-vitest": "^9.0.12",
    "@storybook/react-vite": "^9.0.12",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.2.3",
    "@vitest/coverage-v8": "3.2.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.12",
    "fake-indexeddb": "^6.1.0",
    "globals": "^16.0.0",
    "http-server": "^14.1.1",
    "playwright": "^1.53.0",
    "storybook": "^9.0.12",
    "ts-essentials": "^10.1.1",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "vitest-browser-react": "^0.3.0",
    "wait-on": "^8.0.3"
  }
}
